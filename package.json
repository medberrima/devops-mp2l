{
  "name": "devops-mp2l",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "lint": "eslint src",
    "test": "jest",
    "snyk-test": "snyk test --all-projects",
    "docker:build:dev": "docker build -f Dockerfile.dev -t mp2l-devops:dev .",
    "docker:build:prod": "docker build -f Dockerfile -t mp2l-devops:prod .",
    "docker:run:dev": "docker run -p 3000:3000 -v $(pwd):/app mp2l-devops:dev",
    "docker:run:prod": "docker run -p 3000:3000 mp2l-devops:prod",
    "docker:compose:dev": "docker-compose up dev",
    "docker:compose:prod": "docker-compose up prod",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "next": "15.1.2",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "babel-jest": "^29.7.0",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.2",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "snyk": "^1.1294.3",
    "typescript": "^5",
    "typescript-eslint": "^8.18.2"
  }
}
